{
  "name": "@zhixingyu/ws-inexus-components",
  "version": "0.0.1",
  "description": "vue3 组件库模板",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "vue3-lib-template",
    "ws-inexus-components",
    "vue",
    "vue3",
    "lib",
    "template",
    "cli"
  ],
  "author": "Mayer",
  "license": "MIT",
  "private": false,
  "types": "index.d.ts",
  "type": "module",
  "files": [
    "lib/*",
    "LICENSE",
    "README.md"
  ],
  "main": "./lib/ws-inexus-components.cjs",
  "module": "./lib/ws-inexus-components.js",
  "browser": "./lib/ws-inexus-components.umd.cjs",
  "style": "./lib/style.css",
  "exports": {
    ".": {
      "import": "./lib/ws-inexus-components.js",
      "require": "./lib/ws-inexus-components.cjs",
      "types": "./lib/index.d.ts"
    },
    "./style.css": {
      "import": "./lib/style.css",
      "require": "./lib/style.css",
      "default": "./lib/style.css"
    }
  },
  "engines": {
    "node": ">= 18.0.0"
  },
  "scripts": {
    "build": "vue-tsc --project ../../tsconfig.json && vite build",
    "release": "release-it"
  },
  "dependencies": {
    "echarts": "^5.6.0"
  }
}
